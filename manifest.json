{
  "manifest_version": 2,

  "name": "chrome-extension-skeleton",
  "short_name": "chrome-extension-skeleton",
  "description": "chrome-extension-skeleton",
  "version": "0.1",
  "author": "my-name",

  "browser_action": {
    "default_icon": "resources/images/icon.png",
    "default_popup": "dist/popup.html"
